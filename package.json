{
  "name": "simple-express",
  "version": "1.0.0",
  "description": "This is to create some server... so that I can host my angular application here...",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon app.js",
    "start23": "npm run kill23 && nodemon app.js",
    "kill": "kill -9 $(lsof -i :3044) &>/dev/null | exit 0",
    "kill23": "lsof -i:3044 | awk 'FNR==2 {print $2}' | xargs kill -9",
    "start": "nodemon index.js 3044",
    "use_this_final": "node index.js 3044",
    "stop": "kill $(pidof 'simpleExpress')",
    "status": "$(pidof 'simpleExpress')"
  },
  "author": "GopAL",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.26.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-session": "^1.17.3",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "soap": "^0.8.0"
  },
  "devDependencies": {
    "kill-port": "^1.6.1",
    "nodemon": "^2.0.8"
  },
  "nodemonConfig": {
    "delay": "1000"
  },
  "duplicate_nodemonConfig": {
    "watch": [
      "server",
      "app.js"
    ],
    "ext": "ts",
    "ignore": [
      "*.test.ts"
    ],
    "delay": "1000",
    "execMap": {
      "ts": "ts-node"
    }
  }
}
